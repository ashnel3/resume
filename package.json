{
  "name": "@ashnel3/resume",
  "version": "1.0.0",
  "description": "personal resume template",
  "type": "module",
  "scripts": {
    "start": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "format": "prettier -cw .",
    "format-check": "prettier -l .",
    "schema": "typescript-json-schema src/types/context.d.ts Context -o schema/context.json",
    "test": "vitest --run",
    "prepare": "husky && pnpm schema"
  },
  "homepage": "https://github.com/ashnel3/resume#readme",
  "bugs": {
    "url": "https://github.com/ashnel3/resume/issues"
  },
  "author": "ashnel3",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ashnel3/resume.git"
  },
  "license": "MIT",
  "keywords": [
    "resume",
    "resume-template"
  ],
  "dependencies": {
    "@iconify/json": "^2.2.366",
    "@iconify/types": "^2.0.0",
    "@iconify/utils": "^2.3.0",
    "marked": "^16.1.2"
  },
  "devDependencies": {
    "@tailwindcss/vite": "^4.1.11",
    "@types/node": "^24.2.0",
    "husky": "^9.1.7",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "tailwindcss": "^4.1.11",
    "typescript": "^5.9.2",
    "vite": "^7.0.6",
    "vite-plugin-ejs": "^1.7.0",
    "vite-plugin-singlefile": "^2.3.0",
    "vitest": "^3.2.4"
  },
  "engines": {
    "node": ">= 22"
  },
  "engineStrict": true,
  "pnpm": {
    "onlyBuiltDependencies": [
      "@tailwindcss/oxide",
      "esbuild"
    ]
  }
}
